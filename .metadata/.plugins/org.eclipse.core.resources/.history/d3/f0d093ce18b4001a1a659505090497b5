#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int max(int int1, int int2);

int main(void) {
	char buffer[17] = "The length is 16";

	printf("sizeof( buffer ) is: %lu\n", sizeof(buffer));
	printf("sizeof( buffer ) is: %lu\n", strlen(buffer));

	printf("(*max)(49,50) is: %d\n", (*max)(49,50));

	int theMaxIs = (*max)(49,50);
	int *yPtr = &theMaxIs;
	int **y;
}
